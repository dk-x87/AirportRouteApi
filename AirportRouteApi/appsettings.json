{
  "Data": {
    "RouteUri": "https://homework.appulate.com/api/Route/outgoing?airport=",
    "AirportUri": "https://homework.appulate.com/api/Airport/search?pattern=",
    "AirlineUri": "https://homework.appulate.com/api/Airline/",
    "MaxConcurrentRequests": 50,
    "MaxTransferCount": 2,
    "MaxRequestCount": 2,
    "LogsFile": "Logs/AirportRouteApi-{Date}.txt",
    "ExceptionHandler": "/api/routes/error"
  },
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  }
}
